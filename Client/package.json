    "email": "info@3dkrender.com",
    "url": "https://3dkrender.com",
    "github": "https://github.com/3dkrender",
    "twitter": "https://twitter.com/MarcoS3DK"
  },
  "version": "0.8.0",
  "description": "A client web template with React, Redux, UAL, NextUI, Typescript, Vite",
  "keywords": [],
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "dev:tlocal": "vite --open --mode testnet-local",
    "dev:testnet": "vite --open --mode testnet-production",
    "start:mlocal": "vite --open --mode production-local",
    "start:main": "vite --open --host --mode production",
    "build:mainnet": "tsc && vite build --mode production",
    "build:testnet": "tsc && vite build --mode testnet-production"
  },
  "dependencies": {
    "@heroui/react": "2.7.2",
    "@reduxjs/toolkit": "^2.2.4",
    "@types/lodash": "^4.14.195",
    "@wharfkit/protocol-scatter": "^1.3.1",
    "@wharfkit/session": "^1.0.0",
    "@wharfkit/wallet-plugin-anchor": "^1.0.0",
    "@wharfkit/wallet-plugin-cloudwallet": "^1.0.0",
    "@wharfkit/wallet-plugin-wombat": "^1.1.0",
    "@wharfkit/web-renderer": "^1.0.0",
    "axios": "^1.4.0",
    "framer-motion": "12.4.7",
    "i18next": "^23.11.4",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.15.0",
    "redux": "^5.0.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.14",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^26.0.0",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.8"
  }
}
